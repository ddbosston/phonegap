<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:cdv="http://cordova.apache.org/ns/1.0"
    id        = "com.Pruebapush.android"
    version   = "1.0.0">



<name>Pruebapush</name>

<description>
Aplicaci√≥n de prueba para mensajes PUSH
</description>

<author href="https://www.davston.com" email="ddbosston@gmil.com">

</author>

<preference name='phonegap-version' value='cli-7.0.1' />
<preference name='pgb-builder-version' value='1' />
 
<plugin name="cordova-plugin-camera" spec="2.2.0" />
<plugin name="cordova-plugin-contacts" spec="~3.0.0" />
<plugin name="cordova-plugin-device" spec="~1.1.7"/>
<plugin name="cordova-plugin-device-motion" spec="~2.0.0" />
<plugin name="cordova-plugin-dialogs" spec="~1.3.4" />
<plugin name="cordova-plugin-globalization"/>
<plugin name="cordova-plugin-media" spec="~4.0.0"/>
<plugin name="cordova-plugin-whitelist" spec="~1.3.3" />

<plugin name="https://github.com/phonegap-build/PushPlugin.git"/>

<plugin name="phonegap-plugin-push" source="npm"  spec="1.9.2">
  <variable name="SENDER_ID" value="957877979286" />
</plugin>

<plugin name="cordova-plugin-badge" source="npm" />



<plugin name="cordova-plugin-firebase" spec="0.1.20" />

<platform name="android">
  <content src="index2_1.html" />
  <resource-file src="google-services.json" target="google-services.json" />
</platform>



<access origin="*" />



</widget>
